html
    head
        title #{questionPaper.title}
        link(rel="stylesheet" type="text/css" href="./createQuestionPaper/create_question_paper.css")
    body(class='paperContent')
        h1(class="questionPaperTitle") #{questionPaper.title}
        each question,index in questionPaper.questions
            p
                b #{index+1}.
                | &nbsp !{question.question.replace('<code>','<pre><code>').replace('</code>','</pre></code>')}

