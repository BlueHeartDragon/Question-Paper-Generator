script(src="./createQuestionPaper/create_question_paper_view.js")
link(rel="stylesheet" type="text/css" href="./dashboard/dashboard.css")
link(rel="stylesheet" type="text/css" href="./createQuestionPaper/create_question_paper.css")
link(rel="stylesheet" type="text/css" href="../library/taggle/assets/css/taggle.css")
div(id="inner-container" class="inner-container fullHeight")
 button(id="create_questions",type="button" ,class="btn btn-primary btn-lg",data-toggle="modal", data-target="#myModal") + Question
 div(class="modal fade",id="myModal",role="dialog")
 div(id="questionsToSelect")
  div(id='question_heading') Questions
  br
  div(class="input-group col-md-12")
   div(class="input textarea tagbox" id="tagsInput")
  div(id="all_question_container")
  button(id="addQuestions", type="button" ,class="btn btn-primary btn-lg") Add
 div(id="questionPaper")
  div(id="questionPaperContents")
   div(id="header")
    div(class="titleHolder")
     input(type='text' id="questionPaperTitle" placeholder="Untitled Paper")
    div(class="noOfQuestionHolder")
     label(class="totalQuestionLabel") Total questions:
     div(id="totalQuestions") 0
   div(id="body")
  div(id="footer")
   button(id="preview_button", type="button" ,class="btn btn-primary btn-lg") Preview
   button(type="button" id="save" class="btn btn-primary btn-lg") Save
script(src="../library/taggle/assets/js/jquery-1.10.1.js")
script(src="../library/taggle/assets/js/jquery-ui.js")
script(src="../library/taggle/assets/js/taggle.js")
