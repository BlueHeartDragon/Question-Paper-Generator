link(rel="stylesheet" type="text/css" href="../css/paper_list.css")
link(rel="stylesheet" type="text/css" href="./dashboard.css")
script(src="./question_paper_to_show_view.js")
if questionPapers.length == 0
    div(id='no_question_paper_container')
        div(id='noQuestionPaperAvailable') No Papers Available
else
    div(id='question_paper_container')
        each qp in questionPapers
            div(class="col-xs-12 jumbotron question_paper_title" id='#{qp.id}') #{qp.header.title}