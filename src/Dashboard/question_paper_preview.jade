html
 head
 link(rel="stylesheet" type="text/css" href="./createQuestionPaper/create_question_paper.css")
 body(class='paperContent')
  h1(class="questionPaperTitleInPreview") #{title}
  ol
   each question,index in questions
    div !{notes[index]}
    li !{question.question.replace('<code>','<pre><code>').replace('</code>','</pre></code>')}
